{
  "expo": {
    "name": "RunFlow",
    "slug": "RunFlow",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.runflow.app",
      "config": {
        "googleMapsApiKey": "AIzaSyDMtb3jPziMeVjTckpARxwU9VvuewuHCHc"
      },
      "infoPlist": {
        "NSHealthShareUsageDescription": "RunFlow needs access to read your health data to provide personalized insights and track your fitness progress",
        "NSHealthUpdateUsageDescription": "RunFlow needs access to save your workouts to Apple Health",
        "UIBackgroundModes": ["remote-notification", "location"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDMtb3jPziMeVjTckpARxwU9VvuewuHCHc"
        }
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.POST_NOTIFICATIONS"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#f97316"
    },
    "plugins": [
      "expo-router",
      "expo-sqlite",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#f97316"
        }
      ],
      [
        "react-native-health",
        {
          "isClinicalDataEnabled": false
        }
      ]
    ]
  }
}
